{
    "name": "leaselinkorg/ll-php-lib",
    "version": "1.0.1",
    "homepage": "https://github.com/leaselinkorg/ll-php-lib",
    "description": "PHP library for LeaseLink API integration",
    "keywords": ["leaselink", "api", "integration"],
    "type": "library",
    "require": {
        "php": ">=8.1",
        "psr/log": "^2.0 || ^3.0",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "license": "GPL-3.0",
    "autoload": {
        "psr-4": {
            "LeaseLink\\": "src/"
        }
    },
    "authors": [
        {
            "name": "LeaseLink",
            "email": "integracje@leaselink.pl"
        }
    ],
    "support": {
        "issues": "https://github.com/leaselinkorg/ll-php-lib/issues",
        "source": "https://github.com/leaselinkorg/ll-php-lib"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
